#include <stdio.h>
//Vinicius Mendes UC22101690

int main() {
	
	int n1=0;
	
	printf("Informe um numero: ");
	scanf("%d", &n1);
	
	if(n1 >= 100 && n1 <= 200){
		printf("Ele esta no intervalo entre 100 e 200! %d", n1);
	}else{
		printf("Ele nao esta no intervalo entre 100 e 200! %d", n1);
	}
	
	return 0;
}