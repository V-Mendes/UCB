#include <stdio.h>
//Vinicius Mendes UC22101690

int main() {
	
	float salario=0, resultado=0,novosalario=0;
	
	printf("Informe o valor do seu salario /Digite o valor sem pontos e virgulas/: ");
	scanf("%f", &salario);
	
	if(salario <= 1000){
		resultado = salario * 0.1;
		novosalario = salario + resultado;
		printf("O seu salario com aumento de 10 por cento eh: %.2f", novosalario);
	}else{
		printf("O seu salario continua o mesmo: %.2f", salario);
	}
	
	return 0;
}